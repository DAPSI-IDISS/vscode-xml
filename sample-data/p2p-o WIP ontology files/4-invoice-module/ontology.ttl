                                  <http://purl.org/dc/terms/license> "https://opensource.org/licenses/MIT" ;
                                  <http://purl.org/vocab/vann/preferredNamespacePrefix> "p2p-o-inv" ;
                                  <http://purl.org/vocab/vann/preferredNamespaceUri> "https://purl.org/p2p-o/invoice" ;
                                  rdfs:label "Invoice Module of P2P-O"@en .

