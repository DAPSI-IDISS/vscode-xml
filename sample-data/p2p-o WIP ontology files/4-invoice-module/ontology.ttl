###  https://purl.org/p2p-o/invoice#hasPaymentCard
:hasPaymentCard rdf:type owl:ObjectProperty ;
                rdfs:domain <https://purl.org/p2p-o/document#E-Invoice> ;
                rdfs:range :PaymentCard ;
                rdfs:comment "Card used for payment contemporaneous with invoice issuance."@en ;
                rdfs:label "has payment card"@en ;
                <https://purl.org/p2p-o/item#seeEN16931-1-2017> "BG-18" ;
                <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/usageNote> "Only used if the Buyer had opted to pay by using a payment card such as a credit or debit card."@en .


###  https://purl.org/p2p-o/invoice#hasPaymentServiceProvider
:hasPaymentServiceProvider rdf:type owl:ObjectProperty ;
                           rdfs:domain :PaymentAccount ;
                           rdfs:range <https://purl.org/p2p-o/organization#Identifier> ;
                           rdfs:comment "An identifier for the payment service provider where a payment account is located."@en ;
                           rdfs:label "has payment service provider"@en ;
