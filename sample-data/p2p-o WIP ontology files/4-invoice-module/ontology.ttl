                   rdfs:comment "Payment account, at a payment service provider, to which payment should be made."@en ;
                   rdfs:label "has payment account"@en ;
                   <https://purl.org/p2p-o/item#seeEN16931-1-2017> "BG-17" .


###  https://purl.org/p2p-o/invoice#hasPaymentAccountIdentifier
:hasPaymentAccountIdentifier rdf:type owl:ObjectProperty ;
                             rdfs:domain :PaymentAccount ;
                             rdfs:range <https://purl.org/p2p-o/organization#Identifier> ;
                             rdfs:comment "A unique identifier of the financial payment account, at a payment service provider, to which payment should be made."@en ;
