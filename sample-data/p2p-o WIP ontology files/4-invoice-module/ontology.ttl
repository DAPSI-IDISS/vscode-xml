###  https://purl.org/p2p-o/invoice#deliverToLocation
:deliverToLocation rdf:type owl:ObjectProperty ;
                   rdfs:domain <https://purl.org/p2p-o/document#E-Invoice> ;
                   rdfs:range <https://purl.org/p2p-o/organization#Identifier> ;
                   rdfs:comment "An identifier for the location at which the goods and services are delivered."@en ;
                   rdfs:label "deliver to location"@en ;
                   <https://purl.org/p2p-o/item#seeEN16931-1-2017> "BT-71" .


###  https://purl.org/p2p-o/invoice#deliverToParty
:deliverToParty rdf:type owl:ObjectProperty ;
                rdfs:domain <https://purl.org/p2p-o/document#E-Invoice> ;
                rdfs:range <http://www.w3.org/ns/org#FormalOrganization> ;
                rdfs:comment "To which the goods and services are delivered."@en ;
                rdfs:label "deliver to party"@en ;
                <https://purl.org/p2p-o/item#seeEN16931-1-2017> "BG-13" ,
                                                                "BT-70" ;
                <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/usageNote> "Shall be used if the Deliver to party is different from the Buyer."@en .


###  https://purl.org/p2p-o/invoice#followsInvoice
:followsInvoice rdf:type owl:ObjectProperty ;
                owl:inverseOf :precedesCreditNote ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain :E-CreditNote ;
                rdfs:range <https://purl.org/p2p-o/document#E-Invoice> ;
                rdfs:label "follows invoice"@en .


###  https://purl.org/p2p-o/invoice#hasAllowanceAmount
:hasAllowanceAmount rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( <https://purl.org/p2p-o/document#E-Invoice>
                                                <https://purl.org/p2p-o/documentline#InvoiceLine>
                                              )
                                ] ;
                    rdfs:range <https://purl.org/p2p-o/price#AllowanceAmountOfMoney> ;
                    rdfs:label "has allowance amount"@en ;
                    <https://purl.org/p2p-o/item#seeEN16931-1-2017> "BG-20" ,
                                                                    "BG-27" ,
                                                                    "BT-136" ,
                                                                    "BT-92" .


###  https://purl.org/p2p-o/invoice#hasAllowanceBaseAmount
:hasAllowanceBaseAmount rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
